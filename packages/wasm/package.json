{
	"name": "@nuru/wasm",
	"version": "0.5.19",
	"description": "A wasm compiler/intepreter for [Nuru](https://github.com/NuruProgramming/Nuru)",
	"main": "dist/index.js",
	"files": [
		"dist",
		"main.wasm"
	],
	"scripts": {
		"setup": "bash setup.sh",
		"test": "echo \"Mocktest Wasm-core  游릭 游릭 游릭 游릭\" && exit 0 >> /dev/null",
		"build:wasm": "pnpm run replace && GOOS=js GOARCH=wasm tinygo build -o main.wasm",
		"build": "tsc",
		"prepublishOnly": "npm run build:wasm && npm run build",
		"dev": "tsc --watch & go run cmd/server/main.go",
		"replace": "cp -r ./modified/* ./deps/Nuru/"
	},
	"type": "module",
	"man": "./dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [],
	"author": "Nehemia Kaaya <heracraft@teksafari.org>",
	"license": "ISC",
	"dependencies": {
		"@types/golang-wasm-exec": "^1.15.2",
		"xss": "^1.0.15"
	},
	"devDependencies": {
		"@types/node": "^22",
		"typescript": "^5.9.3"
	}
}
