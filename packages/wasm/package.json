{
	"name": "@nuru/wasm",
	"version": "1.0.0",
	"private": true,
	"description": "A wasm compiler/intepreter for [Nuru](https://github.com/NuruProgramming/Nuru)",
	"main": "wasm_exec.js",
	"scripts": {
		"setup": "go mod vendor",
		"test": "echo \"Mocktest Wasm-core  游릭 游릭 游릭 游릭\" && exit 0 >> /dev/null",
		"vendor": "GOOS=js GOARCH=wasm go build -mod=vendor -o main.wasm",
		"build": "tsc",
		"dev": "tsc --watch",
		"replace": "cp -r ./modified/* ./vendor/github.com/NuruProgramming/Nuru/"
	},
	"type": "module",
	"man": "./dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [],
	"author": "Nehemia Kaaya <heracraft@teksafari.org>",
	"license": "ISC",
	"dependencies": {
		"@types/golang-wasm-exec": "^1.15.2"
	},
	"devDependencies": {
		"@types/node": "^22",
		"typescript": "^5.9.3",
		"xss": "^1.0.15"
	}
}
